{
  "mcpServers": {
    "chrome-dev-tools": {
      "command": "npx",
      "args": ["-y", "chrome-devtools-mcp@latest"]
    },
    "sentry-spotlight": {
      "command": "npx",
      "args": ["-y", "@spotlightjs/spotlight", "--stdio-mcp"]
    },
    "sentry": {
      "url": "https://mcp.sentry.dev/mcp",
      "transport": "http"
    },
    "agentsfera": {
      "url": "https://api.agentsfera.ai",
      "transport": "http"
    }
  }
}
